<h1>Listing tickers</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Code Bloomberg</th>
    <th>Format</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tickers.each do |ticker| %>
  <tr>
    <td><%= ticker.code %></td>
    <td><%= ticker.code_bloomberg %></td>
    <td><%= ticker.format %></td>
    <td><%= link_to 'Show', ticker %></td>
    <td><%= link_to 'Edit', edit_ticker_path(ticker) %></td>
    <td><%= link_to 'Destroy', ticker, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

